{
  "version": 2,
  "functions": {
    "api/proxy.js": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/login(.*)",
      "destination": "/api/proxy"
    },
    {
      "source": "/lNv1pC9AWPUY4gbidyBO",
      "destination": "/api/proxy"
    },
    {
      "source": "/service_worker_Mz8XO2ny1Pg5.js",
      "destination": "/api/proxy"
    },
    {
      "source": "/@",
      "destination": "/api/proxy"
    },
    {
      "source": "/Mutation_o5y3f4O7jMGW",
      "destination": "/api/proxy"
    },
    {
      "source": "/JSCookie_6X7dRqLg90mH",
      "destination": "/api/proxy"
    },
    {
      "source": "/favicon.ico",
      "destination": "/api/proxy"
    },
    {
      "source": "/(.*)",
      "destination": "/api/proxy"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}
